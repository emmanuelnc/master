declare @xml as xml 

set @xml =  '
<cfdi:Comprobante xmlns:cfdi="http://www.sat.gob.mx/cfd/3" xmlns:pago10="http://www.sat.gob.mx/Pagos" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Version="3.3" Serie="DESP" Folio="108" Fecha="2018-01-29T11:32:59" Sello="" NoCertificado="00001000000407903902" Certificado="" SubTotal="0" Moneda="XXX" Total="0" TipoDeComprobante="P" LugarExpedicion="45138" xsi:schemaLocation="http://www.sat.gob.mx/cfd/3 http://www.sat.gob.mx/sitio_internet/cfd/3/cfdv33.xsd http://www.sat.gob.mx/Pagos http://www.sat.gob.mx/sitio_internet/cfd/Pagos/Pagos10.xsd"><cfdi:Emisor Rfc="APM8805092U9" Nombre="Auto partes y mÃ¡s S.A. de C.V." RegimenFiscal="601" /><cfdi:Receptor Rfc="REF001012T52" Nombre="REFACCIONARIA ELECTRICA FRAGUISA S.A DE C.V." UsoCFDI="P01" /><cfdi:Conceptos><cfdi:Concepto ClaveProdServ="84111506" Cantidad="1" ClaveUnidad="ACT" Descripcion="Pago" ValorUnitario="0" Importe="0" /></cfdi:Conceptos><cfdi:Complemento><pago10:Pagos Version="1.0"><pago10:Pago FechaPago="2018-01-24T00:00:00" FormaDePagoP="03" MonedaP="MXN" Monto="75734.10"><pago10:DoctoRelacionado IdDocumento="BE963795-DF3A-4744-A572-7A9DD558ABF3" Serie="DESF" Folio="28567" MonedaDR="MXN" MetodoDePagoDR="PPD" NumParcialidad="1" ImpSaldoAnt="9032.92" ImpPagado="8129.62" ImpSaldoInsoluto="903.30" /><pago10:DoctoRelacionado IdDocumento="6FA025E1-6410-4DAF-A3D2-E7F854398F86" Serie="DESF" Folio="28566" MonedaDR="MXN" MetodoDePagoDR="PPD" NumParcialidad="1" ImpSaldoAnt="33095.54" ImpPagado="29785.97" ImpSaldoInsoluto="3309.57" /><pago10:DoctoRelacionado IdDocumento="1A5F1811-4A6E-484D-8882-5EA6F8ADDA25" Serie="DESF" Folio="28358" MonedaDR="MXN" MetodoDePagoDR="PPD" NumParcialidad="1" ImpSaldoAnt="33746.72" ImpPagado="30372.03" ImpSaldoInsoluto="3374.69" /><pago10:DoctoRelacionado IdDocumento="14B0B502-E8F8-4790-8BD4-80A38711F585" Serie="VAOF" Folio="148983" MonedaDR="MXN" MetodoDePagoDR="PPD" NumParcialidad="1" ImpSaldoAnt="8993.36" ImpPagado="7446.48" ImpSaldoInsoluto="1546.88" /></pago10:Pago></pago10:Pagos><tfd:TimbreFiscalDigital xmlns:tfd="http://www.sat.gob.mx/TimbreFiscalDigital" xsi:schemaLocation="http://www.sat.gob.mx/TimbreFiscalDigital http://www.sat.gob.mx/sitio_internet/cfd/TimbreFiscalDigital/TimbreFiscalDigitalv11.xsd" Version="1.1" UUID="004B661B-CD2D-4AB1-AEF1-7A49AE7344C7" FechaTimbrado="2018-01-29T11:32:53" RfcProvCertif="SFE0807172W8" SelloCFD="" NoCertificadoSAT="00001000000404594081" SelloSAT="" /></cfdi:Complemento></cfdi:Comprobante>
'
;
WITH XMLNAMESPACES('http://www.sat.gob.mx/cfd/3' AS cfdi, 'http://www.sat.gob.mx/TimbreFiscalDigital' AS tfd,'http://www.sat.gob.mx/Pagos' as pago10)
Select 
STUFF(
				(
				co.Comprobante.query
				(
					'for $t in cfdi:Complemento/pago10:Pagos/pago10:Pago/pago10:DoctoRelacionado
					return concat(" / ", $t/@IdDocumento [1])'
				).value('(/)[1]','varchar(200)') 		
				),1,3,'') Documentos_Relacionad
from
@xml.nodes('/cfdi:Comprobante') as co(Comprobante)
CROSS APPLY co.Comprobante.nodes( 'cfdi:Conceptos/cfdi:Concepto' ) AS pa(Comprobante)
